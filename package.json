{
  "name": "frourio-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run migrate:dev && run-p dev:*",
    "dev:client": "next dev -p 8000",
    "dev:server": "npm run dev --prefix server",
    "dev:aspida": "aspida --watch",
    "build": "run-p build:client build:server",
    "prebuild:client": "npm run generate",
    "build:client": "next build && next export",
    "build:server": "npm run build --prefix server",
    "generate": "run-p generate:*",
    "generate:aspida": "aspida",
    "generate:server": "npm run generate --prefix server",
    "lint": "eslint --ext .ts,.js,.tsx .",
    "lint:fix": "npm run lint -- --fix",
    "migrate:dev": "npm run migrate:dev --prefix server",
    "migrate:reset": "npm run migrate:reset --prefix server",
    "start": "run-p start:*",
    "start:client": "next start",
    "start:server": "npm start --prefix server",
    "pretest": "npm run generate",
    "test": "jest",
    "pretypecheck": "npm run generate",
    "typecheck": "tsc --noEmit && tsc --noEmit -p server"
  },
  "dependencies": {
    "@aspida/axios": "^1.6.3",
    "@aspida/swr": "^1.6.3",
    "axios": "^0.21.1",
    "class-validator": "^0.13.1",
    "next": "^10.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "swr": "^0.4.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/react": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "babel-jest": "^26.6.3",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "fastify": "^3.12.0",
    "fastify-cors": "^5.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.1",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "~4.1.5"
  }
}
